{
    "name": "ghost-times-list",
    "version": "0.0.0",
    "description": "",
    "homepage": "",
    "author": {
        "name": "Water",
        "email": "walker@walkerknapp.me",
        "url": ""
    },
    "scripts": {
        "build": "concurrently \"npm:build:*\"",
        "build:extension": "tsc src/extension/index.ts --skipLibCheck --target es2015 --module CommonJS --moduleResolution node --outDir extension",
        "build:graphics": "vite build src/graphics",
        "build:dashboard": "vite build src/dashboard"
    },
    "files": [
        "dashboard",
        "graphics",
        "extension.js",
        "extension"
    ],
    "keywords": [
        "nodecg-bundle"
    ],
    "nodecg": {
        "compatibleRange": "^1.1.1",
        "dashboardPanels": [
            {
                "name": "ghost-control",
                "title": "Ghost Control",
                "width": 4,
                "file": "ghost-control.html",
                "headerColor": "#525F78"
            },
            {
                "name": "map-control",
                "title": "Map Control",
                "width": 2,
                "file": "map-control.html",
                "headerColor": "#525F78"
            }
        ],
        "graphics": [
            {
                "file": "index.html",
                "width": 1920,
                "height": 1080
            }
        ]
    },
    "repository": "https://github.com/nodecg/nodecg.git",
    "license": "Apache-2.0",
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.3",
        "concurrently": "^7.3.0",
        "svelte-preprocess": "^4.10.7",
        "typescript": "^4.8.2",
        "vite": "^3.0.9",
        "vite-plugin-fonts": "^0.6.0",
        "vite-tsconfig-paths": "^3.5.0"
    },
    "dependencies": {
        "@fortawesome/free-solid-svg-icons": "^6.2.0",
        "smart-buffer": "^4.2.0",
        "svelte-fa": "^3.0.3"
    }
}
